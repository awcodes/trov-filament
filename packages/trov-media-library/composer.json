{
    "name": "trov/media-library",
    "description": "A media library for filament forms",
    "keywords": [
        "laravel",
        "filament",
        "forms",
        "media library"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/forms": "^2.0",
        "illuminate/contracts": "^8.73|^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Trov\\MediaLibrary\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trov\\MediaLibrary\\TrovMediaLibraryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}